{
  "name": "server-ferma-docker",
  "version": "0.0.1",
  "main": "server.js",
  "license": "ISC",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nodemon --legacy-watch ./src/server.ts",
    "devlint": "cross-env NODE_ENV=dev nodemon ./src/server.ts --exec \"yarn lint && node\"",
    "prod": "cross-env NODE_ENV=prod nodemon ./src/server.ts",
    "start": "cross-env NODE_ENV=prod node ./dist/src/server.js",
    "init": "npx tsc --init",
    "build": "cross-env NODE_ENV=prod tsc --build",
    "pm2": "pm2 start cross-env NODE_ENV=dev nodemon ./src/server.ts",
    "pm3": "pm2 start ts-node -- -P tsconfig.json ./src/server.ts",
    "lint": "eslint .",
    "copy-env": "mkdir ./dist/src/env/ && cpx ./src/env/** ./dist/src/env/",
    "copy-env2": "cp -r ./src/env/ ./dist/src/env/"
  },
  "engines": {
    "node": ">=22.0.0 <=22.5.1"
  },
  "dependencies": {
    "@netlify/functions": "^2.8.0",
    "@types/nodemailer": "^6.4.15",
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.6",
    "crypto": "^1.0.1",
    "debug": "4.3.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "~1.10.0",
    "winston": "~3.13.1",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.14.9",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/validator": "^13.12.0",
    "@types/morgan": "^1.9.9",
    "cross-env": "^7.0.3",
    "eslint": "9.x",
    "globals": "^15.8.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.15.0"
  }}
